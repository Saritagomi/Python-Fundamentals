import matplotlib.pyplot as plt
import numpy as np
x=np.array([1,2,3,4])
y=np.array([4,5,6,7])
plt.subplot(1,4,1)
plt.plot(x,y)
x=np.array([3,7,5,4])
y=np.array([6,4,3,6])
plt.subplot(1,4,2)
plt.plot(x,y)
plt.show()
x=np.array([3,6,1,4])
y=np.array([3,4,3,6])
plt.subplot(1,4,3)
plt.plot(x,y)
plt.show()

x=np.random.randint(low=1,high=10,size=25)
plt.subplot(1,4,4)
plt.plot(x,color="red")
plt.show()


