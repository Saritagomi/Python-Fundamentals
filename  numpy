import numpy as np
arr=np.array([1,2,3,4])
print(arr)
print(type(arr))
a=np.array(3)
print(a)

arr=np.array([[2,3,4],[3,5,6]])
print(arr)
print(type(arr))
arr=np.array([[[1,2,3],[2,3,4],[4,5,6]]])
print(arr)

ss=np.linspace(0,1,4)
print(ss)
ss=np.linspace(0,1,6)
print(ss)
